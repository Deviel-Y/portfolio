generator client {
  provider     = "prisma-client"
  output       = "./generated/client"
  moduleFormat = "esm"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model skill {
  id        String     @unique @default(uuid())
  name      String     @unique
  skillType Skill_Type
}

enum Skill_Type {
  FRONT_END
  BACK_END
  DEV_OPS
}
